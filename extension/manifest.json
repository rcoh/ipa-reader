{
  "manifest_version": 2,

  "name": "Wikipedia IPA Speaker",
  "description": "This extension allows you to listen to IPA on Wikipedia.",
  "version": "1.3",
  "icons": {
    "64": "favicon.ico"
  },
  "content_scripts": [{
    "matches": ["https://*.wikipedia.org/*"],
    "js": ["jquery-3.2.1.min.js", "ipa.js"],
    "css": ["ipa.css"]
  }],
  "web_accessible_resources": ["play-button.svg"]
}